#include<stdio.h>
#include<conio.h>
void main ()
{
char c;
file*f,*g;
f=fopen ("vivek.txt","r");
if (f==null)
{
printf("error");
exit(0);
}
fopen("vivek1.txt","w");
if (g=null)
{
printf("error");
exit(0);
}
while (1(f=fgetc(f)!=EOF)
{
put (c,g);
}
printf("sucesfully copied");
fclose(f);
fclose(g);
}
